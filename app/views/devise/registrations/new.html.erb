<div class="about-us cover2">

	<% #TODO - add in user flow for payment & signup for the 200 ul plan %>
	<% #TODO - rename subscriptions in the USERS table - it is mispelled  %>

	<div class="signup">
		<h2>Sign up</h2>

	</div>
			  <h6><%= devise_error_messages! %></h6>

	<div class="center">

		<div class="information">
			<p id="register-headline">Start rating and reviewing your lab experience.</p>
			<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

			  <div><%= f.text_field :first_name, placeholder: "First Name" %></div>

			  <div><%= f.text_field :last_name, placeholder: "Last Name" %></div>

			  <div><%= f.text_field :organization, placeholder: "Organization" %></div>

			  <div><%= f.email_field :email, placeholder: "Email" %></div>

			  <div><%= f.password_field :password, placeholder: "Password" %></div>

			  <div><%= f.password_field :password_confirmation, placeholder: "Confirm Password" %></div>

			  <div><%= f.submit "Sign up", :class => "btn" %></div>
			<% end %>

			<%= render "devise/shared/links" %>
		</div>
	</div>
</div>
