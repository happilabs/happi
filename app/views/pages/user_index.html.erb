<div class="container hero-unit">
  <h5>USER INDEX</h5>
  <table class="table">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Subscription Level</th>
        <th>Organization</th>
        <th>Sign-in Count</th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.subcription_level %></td>
        <td><%= user.organization %></td>
        <td><%= user.sign_in_count %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
