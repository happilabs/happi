<div class="sky-background">
  <div class="container content-container grid group">

  <h1>User Index</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Subscription Level</th>
        <th>Organization</th>
        <th>Sign-in Count</th>
        <th>Last Sign In</th>
        <th>Delete?</th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.subcription_level %></td>
        <td><%= user.organization %></td>
        <td><%= user.sign_in_count %></td>
        <td><%= user.last_sign_in_at.strftime("%b %d, %Y %l:%M%p ") %></td>
        <td><%= button_to 'Delete?', delete_user_path(user.id), :method => 'delete', class: "btn",  :confirm => 'Are you sure?' %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
</div>
