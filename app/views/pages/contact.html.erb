<div class="chicago-sky blue-background group">



	<div class="contact-info group">

		<div class="contact-us" id="contact-margin">
			<h1>CONTACT US</h1>
			<p style="color:#fff;">How can we help you? We're here to listen.</p>
		</div>

		<div class="contact-wrap group">

			<div class="contact-box group">
				<img src="phone.png" alt="contact us via email" class="contact-img">
				<h4>Phone</h4>
				<p>312-569-0161</p>
			</div>

			<div class="contact-box group">
				<img src="google_logo.jpg" alt="find us on Google chat" class="contact-img">
				<h4>Chat</h4>
				<p>Search 'HappiLabs' in your Google chat</p>
			</div>

			<div class="contact-box group">
				<%= link_to (image_tag("/email.png", alt: "email us!", class: "contact-img")), "mailto:hello@happilabs.org"	 %>
				<h4>Email</h4>

				<p><%= link_to 'hello@happilabs.org', "mailto:hello@happilabs.org" %></p>
			</div>

			<div class="contact-box address group">
				<img src="e_mail.png" alt="contact us via email" class="contact-img">
				<h4>Send us a letter</h4>
				<p>2242 W. Harrison St., Mailbox 15
				</p>
				<p><strong>CHICAGO</strong>, IL 60612
				</p>
			</div>

			<div class="contact-box group">
				<img src="twitter_icon.png" alt="contact us happylabs on twitter" class="contact-img">
				<h4>Tweet at us</h4>
				<p><a href="https://twitter.com/happilabs_org">@Happilabs_org</a></p>
				<p>(or see the footer)</p>
			</div>

			<div class="contact-box group">
				<img src="facebook-icon.png" alt="contact HappiLabs via Facebook" class="contact-img">
				<h4>Free karma at</h4>
				<p><a href="https://www.facebook.com/HappiLabs?ref=hl">HappiLabs' Facebook Page</a></p>
			</div>

		</div>

	</div>

</div>


<script>
$(document).ready(function() {
  $('#contact').addClass('active');
});
</script>


